<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/maktab_navigation"
	app:startDestination="@id/maktabHomeFragment">
	<fragment
		android:id="@+id/maktabHomeFragment"
		android:name="com.wordpress.lochindev.a13_maktab.screen.fragment.MaktabHomeFragment"
		android:label="Maktab"
		tools:layout="@layout/fragment_maktab_home" >
		<action
			android:id="@+id/action_maktabHomeFragment_to_maktabHaqidaFragment"
			app:destination="@id/maktabHaqidaFragment" />
		<action
			android:id="@+id/action_maktabHomeFragment_to_hujjatlarFragment"
			app:destination="@id/hujjatlarFragment" />
		<action
			android:id="@+id/yangiliklar2"
			app:destination="@id/yangiliklarFragment2" />
	</fragment>
	<fragment
		android:id="@+id/maktabHaqidaFragment"
		android:name="com.wordpress.lochindev.a13_maktab.screen.fragment.MaktabHaqidaFragment"
		android:label="Maktab haqida"
		tools:layout="@layout/fragment_maktab_haqida" >
		<action
			android:id="@+id/action_maktabHaqidaFragment_to_maktabHomeFragment"
			app:destination="@id/maktabHomeFragment" />
	</fragment>
	<fragment
		android:id="@+id/hujjatlarFragment"
		android:name="com.wordpress.lochindev.a13_maktab.screen.fragment.HujjatlarFragment"
		android:label="Hujjatlar"
		tools:layout="@layout/fragment_hujjatlar" >
		<action
			android:id="@+id/action_hujjatlarFragment_to_maktabHomeFragment"
			app:destination="@id/maktabHomeFragment" />
	</fragment>
	<fragment
		android:id="@+id/yangiliklarFragment2"
		android:name="com.wordpress.lochindev.a13_maktab.screen.fragment.YangiliklarFragment"
		android:label="Yangiliklar"
		tools:layout="@layout/fragment_yangiliklar" />

</navigation>